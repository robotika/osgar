{
  "version": 2,
  "robot": {
    "modules": {
      "app": {
          "driver": "application",
          "in": ["emergency_stop", "pose2d", "pose3d", "scan", "rot", "artf",
                 "sim_time_sec", "acc", "origin"],
          "out": ["desired_speed", "pose2d", "artf_xyz", "pose3d", "stdout", "request_origin"],
          "init": {
            "max_speed": 0.5,
            "symmetric": false,
            "virtual_bumper_sec": 60,
            "virtual_bumper_radius": 10.0,
            "virtual_world": true,
            "speed_policy": "conservative",
            "obstacle_influence": 1.4,
            "max_obstacle_distance": 3.0,
            "height_above_ground": 2.0,
            "limit_roll": 90,
            "limit_pitch": 90,
            "return_limit_roll": 90,
            "return_limit_pitch": 90,
            "trace_z_weight": 1.0
          }
      },
      "detector": {
          "driver": "subt.artf_node:ArtifactDetectorDNN",
          "in": ["image", "scan"],
          "out": ["artf", "dropped", "debug_artf", "stdout"],
          "init": {
            "virtual_world": true,
            "fx": 554.25469
          }
      },
      "gas_detector": {
          "driver": "subt.artf_gas:ArtifactGasDetector",
          "in": ["gas_detected"],
          "out": ["artf"],
          "init": {}
      },
      "reporter": {
          "driver": "subt.artf_reporter:ArtifactReporter",
          "in": ["artf_xyz"],
          "out": ["artf_cmd"],
          "init": {
            "repeat_report_sec": 10
          }
      },
      "transmitter": {
          "driver": "zeromq",
          "in": [],
          "out": ["raw"],
          "init": {
            "mode": "PUSH",
            "endpoint": "tcp://localhost:5556"
          }
      },
      "receiver": {
          "driver": "zeromq",
          "in": [],
          "out": ["raw"],
          "init": {
            "mode": "PULL",
            "endpoint": "tcp://localhost:5555"
          }
      },
      "drone": {
          "driver": "subt.drone:Drone",
          "in": ["desired_speed"],
          "out": ["desired_speed_3d"],
          "init": {
          }
      },
      "rosmsg": {
          "driver": "rosmsg",
          "in": ["slot_raw", "desired_speed", "tick", "stdout", "request_origin"],
          "out": ["rot", "acc", "scan", "image", "pose2d", "sim_time_sec", "cmd", "origin", "gas_detected", "orientation"],
          "init": {
            "downsample": 2
          }
      },
      "depth2scan": {
          "driver": "subt.depth2scan:DepthToScan",
          "in": ["depth", "scan"],
          "out": ["scan"],
          "init": {
            "depth_params": {
              "fx": 554.25469,
              "camera_xyz": [0.1, 0.0, -0.08],
              "image_size": [640, 480],
              "principal_point": [320.5, 240.5],
              "vertical_pixel_offset": 16,
              "lidar_fov": 6.283185307179586,
              "lidar_trusted_zone": 2.0,
              "min_z": -0.2,
              "max_z": 0.4,
              "max_slope": -3.14,
              "vertical_diff_limit": 1.4835, "vd_comment": "radians(85)"
            }
          }
      },
      "fromrospy": {
          "driver": "subt.pull:Pull",
	      "init": {
	        "outputs": ["rot", "acc", "orientation", "top_scan", "bottom_scan", "pose3d", "battery_state", "score",
                        "air_pressure", "scan360"]
	      }
      },
      "torospy": {
          "driver": "subt.push:Push"
      },
      "radio": {
          "driver": "subt.radio:Radio",
          "in": ["radio", "pose3d", "artf", "sim_time_sec"],
          "out": ["radio", "artf_xyz"],
          "init": {}
      },
      "offseter": {
        "driver": "subt.offseter:Offseter"
      },
      "marsupial": {
          "driver": "subt.marsupial:Marsupial",
          "in": ["origin", "sim_time_sec"],
          "out": ["detach"],
          "init": {
          }
      },
      "mtm": {
          "driver": "subt.multitrace:MultiTraceManager",
          "in": ["robot_xyz", "trace_info", "robot_trace"],
          "out": ["trace_info", "robot_trace"],
          "init": {}
      }
    },
    "links": [["app.desired_speed", "drone.desired_speed"],
              ["drone.desired_speed_3d", "rosmsg.desired_speed_3d"],
              ["app.stdout", "rosmsg.stdout"],
              ["app.request_origin", "rosmsg.request_origin"],
              ["rosmsg.origin", "app.origin"],

              ["receiver.raw", "rosmsg.raw"],
              ["rosmsg.cmd", "transmitter.raw"],

              ["rosmsg.rot", "app.rot"],
              ["fromrospy.rot", "depth2scan.rot"],
              ["rosmsg.acc", "app.acc"],
              ["rosmsg.orientation", "app.orientation"],

              ["fromrospy.scan360", "detector.scan"],
              ["fromrospy.scan360", "depth2scan.scan"],

              ["rosmsg.image", "detector.image"],
              ["rosmsg.image", "app.image"],

              ["rosmsg.depth", "depth2scan.depth"],
              ["depth2scan.scan", "app.scan360"],
              ["rosmsg.depth", "detector.depth"],

              ["fromrospy.rot", "app.rot"],
              ["offseter.pose3d", "app.pose3d"],
              ["rosmsg.sim_time_sec", "app.sim_time_sec"],
              ["rosmsg.sim_time_sec", "reporter.sim_time_sec"],

              ["rosmsg.gas_detected", "gas_detector.gas_detected"],
              ["gas_detector.artf", "app.artf"],

              ["offseter.pose3d", "radio.pose3d"],
              ["reporter.artf_all", "radio.artf"],
              ["radio.artf_xyz", "reporter.artf_xyz"],
              ["radio.radio", "rosmsg.broadcast"],
              ["rosmsg.radio", "radio.radio"],
              ["rosmsg.sim_time_sec", "radio.sim_time_sec"],

              ["radio.robot_xyz", "mtm.robot_xyz"],
              ["radio.trace_info", "mtm.trace_info"],
              ["radio.robot_trace", "mtm.robot_trace"],
              ["mtm.trace_info", "radio.trace_info"],
              ["mtm.robot_trace", "radio.robot_trace"],

              ["rosmsg.sim_time_sec", "marsupial.sim_time_sec"],
              ["rosmsg.origin", "marsupial.origin"],
              ["marsupial.detach", "torospy.detach"],

              ["detector.artf", "app.artf"],
              ["detector.stdout", "rosmsg.stdout"],
              ["app.artf_xyz", "reporter.artf_xyz"],
              ["rosmsg.base_station", "reporter.base_station"],
              ["reporter.artf_cmd", "transmitter.raw"],

              ["fromrospy.bottom_scan", "drone.bottom_scan"],
              ["fromrospy.top_scan", "drone.top_scan"],
              ["offseter.pose3d", "drone.pose3d"],
              ["fromrospy.air_pressure", "drone.air_pressure"],
              ["app.desired_z_speed", "drone.desired_z_speed"],

              ["rosmsg.origin", "offseter.origin"],
              ["fromrospy.pose3d", "offseter.pose3d"]
    ]
  }
}
