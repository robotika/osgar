{
  "version": 2,
  "robot": {
    "modules": {
      "app": {
          "driver": "osgar.go:Go",
          "in": ["pose2d", "scan"],
          "out": ["desired_speed"],
          "init": {
            "max_speed": 1.0,
            "desired_angle": 0,
            "dist": 1,
            "timeout": 10
          }
      },
      "gym": {
          "driver": "examples.gym.gym_simulator:GymSimulator",
          "in": ["desired_speed", "tick"],
          "out": ["pose2d", "tick"],
          "init": {
            "map_config": "config_example_map.yaml"
          }
      },
      "tick": {
          "driver": "timer",
          "in": [],
          "out": ["tick"],
          "init": {
            "sleep": 0.02
          }
      }
    },
    "links": [["app.desired_speed", "gym.move"],
              ["tick.tick", "gym.tick"],
              ["gym.pose2d", "app.pose2d"]]
  }
}
