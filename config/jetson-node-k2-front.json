{
  "version": 2,
  "robot": {
    "modules": {
      "camera": {
          "driver": "http",
          "in": [],
          "out": ["raw"],
          "init": {
              "url": "http://192.168.0.32/image?res=half",
              "sleep": 0.2,
              "timeout": 1.0
          }
      },
      "detector":{
        "driver": "subt.jetson_artf_node:ArtifactDetectorJetson",
        "in": ["image"],
        "out": ["localized_artf", "dropped", "debug_image", "debug_result"],
        "init": {}

      },
      "transmitter": {
          "driver": "subt.push_bind:PushBind",
          "init": {
            "endpoint": "tcp://*:5557"
          }
      },
      "T200": {
          "driver": "realsense",
          "init": {
            "serial_number": "929122111204",
            "device": "T200",
            "pose_subsample": 20
          }
      },
      "D435_slope": {
          "driver": "realsense",
          "init": {
            "serial_number": "939622072545",
            "device": "D400",
            "depth_subsample": 1,
            "depth_rgb": true,
            "depth_resolution": [640, 360],
            "rgb_resolution": [640, 360],
            "depth_fps": 6
          }
      }
    },
    "links": [
      ["camera.raw", "detector.image"],
      ["detector.localized_artf", "transmitter.localized_artf"],
      ["detector.debug_image", "transmitter.debug_image" ],
      ["T200.pose3d", "detector.pose3d"],
      ["T200.pose3d", "transmitter.pose3d"],
      ["D435_slope.dept", "transmitter.depth"]
    ]
  }
}
