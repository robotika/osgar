{
  "version": 2,
  "robot": {
    "modules": {
      "jd": {
          "driver": "johndeere",
          "in": ["can"],
          "out": ["can"],
          "init": {}
      },
      "can": {
          "driver": "can",
          "in": ["raw", "can"],
          "out": ["can", "raw"],
          "init": {"speed": 250000, "canopen":true}
      },
      "serial": {
          "driver": "serial",
          "in": ["raw"],
          "out": ["raw"],
          "init": {"port": "/dev/ttyS0", "speed": 115200,
                   "rtscts":true, "reset":true}
      },
      "gps": {
          "driver": "gps",
          "in": ["raw"],
          "out": ["position"],
          "init": {}
      },
      "gps_serial": {
          "driver": "serial",
          "in": [],
          "out": ["raw"],
          "init": {"port": "/dev/ttyUSB0", "speed": 4800}
      },
      "dgps": {
          "driver": "gps",
          "in": ["raw"],
          "out": ["position"],
          "init": {}
      },
      "dgps_serial": {
          "driver": "serial",
          "in": [],
          "out": ["raw"],
          "init": {"port": "/dev/ttyACM0", "speed": 115200}
      }
    },
    "links": [["serial.raw", "can.raw"], 
              ["can.raw", "serial.raw"],
              ["jd.can", "can.can"],
              ["can.can", "jd.can"],
              ["gps_serial.raw", "gps.raw"],
              ["dgps_serial.raw", "dgps.raw"]]
  }
}
