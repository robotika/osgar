{
  "version": 2,
  "robot": {
    "modules": {
      "app": {
          "driver": "application",
          "in": ["emergency_stop", "pose2d"],
          "out": ["desired_speed"],
          "init": {
            "init_path": "2.5,0",
            "max_speed": 0.5,
            "symmetric": true,
            "direction_adherence": 45,
            "obstacle_influence": 1.4,
            "speed_policy": "conservative",
            "min_safe_dist": 0.8,
            "dangerous_dist": 0.5,
            "rotation_p": 1.6,
            "virtual_bumper_sec": 75,
            "virtual_bumper_radius": 10.0,
	    "virtual_world": true,
            "follow_wall": {
              "max_wall_distance": 5.0
            }
          }
      },
      "cortexpilot": {
          "driver": "osgar.drivers.cortexpilot:Cortexpilot",
          "in": ["raw", "desired_speed"],
          "out": ["raw", "encoders", "emergency_stop", "pose2d", "scan"],
          "init": {}
      },
      "serial": {
          "driver": "serial",
          "in": ["raw"],
          "out": ["raw"],
          "init": {"port": "/dev/ttyACM0", "speed": 115200}
      },
      "depth": {
        "driver": "realsense",
        "init": {
          "device": "D400",
          "serial_number": "848312070179",
          "depth_subsample": 1,
          "depth_rgb": true,
          "depth_resolution": [
            640,
            480
          ],
          "rgb_resolution": [
            640,
            480
          ],
          "depth_fps": 6
        }
      },
      "depth_rear": {
        "driver": "realsense",
        "init": {
          "device": "D400",
          "serial_number": "938422071072",
          "depth_subsample": 1,
          "depth_rgb": true,
          "depth_resolution": [
            640,
            480
          ],
          "rgb_resolution": [
            640,
            480
          ],
          "depth_fps": 6
        }
      },
      "tracking": {
          "driver": "realsense",
          "init": {
            "device": "T200",
            "serial_number": "2322110762",
            "pose_subsample": 20
          }
      },
      "lidar": {
          "driver": "lidar",
          "in": ["raw"],
          "out": ["raw", "scan"],
          "init": {"sleep": 0.1, "mask": [45, -45], "blind_zone": 100}
      },
      "lidar_tcp": {
          "driver": "tcp",
          "in": ["raw"],
          "out": ["raw"],
          "init": {"host": "192.168.1.70", "port": 2111, "timeout": 0.5}
      },
      "lidar_back": {
          "driver": "lidar",
          "in": ["raw"],
          "out": ["raw", "scan"],
          "init": {"sleep": 0.1, "mask": [45, -45], "blind_zone": 100}
      },
      "lidar_back_tcp": {
          "driver": "tcp",
          "in": ["raw"],
          "out": ["raw"],
          "init": {"host": "192.168.1.71", "port": 2111, "timeout": 0.5}
      },
      "bundler": {
          "driver": "subt.bundle_rgbd:Bundler",
          "in": ["robot_pose", "img", "depth"],
          "out": ["rgbd"],
          "init": {
            "camera": {
              "xyz": [0.0, -0.0175, 0.03],
              "ypr": [0.0, 0.0, 0.0]
            },
            "rgb": {
              "w": 640,
              "h": 480,
              "fx": 614.595886230469,
              "fy": 614.745910644531,
              "cx": 320.756683349609,
              "cy": 245.478973388672
            },
            "depth": {
              "w": 640,
              "h": 480,
              "fx": 382.348602294922,
              "fy": 382.348602294922,
              "cx": 323.737854003906,
              "cy": 238.932739257812
            },
            "extrinsics": [
              [0.999967, -0.000261684, 0.00818068, 0.0149064091965556],
              [0.000281749, 0.999997, -0.00245158, 0.000188795995200053],
              [-0.00818002, 0.0024538, 0.999964, 0.000308448070427403]
            ]
          }
      },
      "small": {
        "driver": "resize",
        "in": ["depth"],
        "out": ["depth"],
        "init": {
          "width": 80,
          "height": 60
        }
      },
      "small_rear": {
        "driver": "resize",
        "in": ["depth"],
        "out": ["depth"],
        "init": {
          "width": 80,
          "height": 60
        }
      },
      "fromros": {
          "driver": "subt.pull:Pull",
          "out": "scan360",
          "init": {
              "outputs": ["scan360"]
          }
      },
      "ros": {
        "driver": "subt.roslaunch:ROSLaunch",
        "in": [],
        "out": [],
        "init": {
          "launch_file": "subt/ros/robot/launch/deedee.launch"
        }
      },
      "toros": {
          "driver": "subt.push:Push"
      }
    },
    "links": [["app.desired_speed", "cortexpilot.desired_speed"],
              ["cortexpilot.pose2d", "app.pose2d"],
              ["cortexpilot.emergency_stop", "app.emergency_stop"],
              ["serial.raw", "cortexpilot.raw"], 
              ["cortexpilot.raw", "serial.raw"],
              ["lidar_tcp.raw", "lidar.raw"],
              ["lidar.raw", "lidar_tcp.raw"],
              ["lidar_back_tcp.raw", "lidar_back.raw"],
              ["lidar_back.raw", "lidar_back_tcp.raw"],
              ["lidar.scan", "app.scan"],
              ["lidar_back.scan", "app.scan_back"],
              ["tracking.pose3d", "app.pose3d"],
              ["tracking.pose3d", "bundler.robot_pose"],
              ["depth.color", "bundler.img"],
              ["depth.depth", "bundler.depth"],
              ["depth.depth", "small.depth"],
              ["depth_rear.depth", "small_rear.depth"],
              ["small.depth", "toros.front_depth"],
              ["small_rear.depth", "toros.rear_depth"],
              ["tracking.pose3d", "toros.tracking_pose"]]
  }
}
