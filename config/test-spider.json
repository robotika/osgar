{
  "version": 2,
  "robot": {
    "modules": {
      "spider": {
          "driver": "spider",
          "in": ["can"],
          "out": ["can"],
          "init": {}
      },
      "can": {
          "driver": "can",
          "in": ["raw", "can"],
          "out": ["can", "raw"],
          "init": {}
      },
      "serial": {
          "driver": "serial",
          "in": ["raw"],
          "out": ["raw"],
          "init": {"port": "/dev/ttyS0", "speed": 115200,
                   "rtscts":true, "reset":true}
      }
    },
    "links": [["spider.can", "can.can"],
              ["can.can", "spider.can"],
              ["serial.raw", "can.raw"], 
              ["can.raw", "serial.raw"]]
  }
}
