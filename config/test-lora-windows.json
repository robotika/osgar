{
  "version": 2,
  "robot": {
    "modules": {
      "lora": {
          "driver": "lora",
          "in": ["raw"],
          "out": ["raw"],
          "init": {
            "sleep": 20
          }
      },
      "lora_serial": {
          "driver": "serial",
          "in": ["raw"],
          "out": ["raw"],
          "init": {"port": "COM35", "speed": 115200}
      }
    },
    "links": [["lora_serial.raw", "lora.raw"],
              ["lora.raw", "lora_serial.raw"]]
  }
}
